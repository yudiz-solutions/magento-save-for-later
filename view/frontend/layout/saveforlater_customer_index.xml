<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <head>
          <css src="Yudiz_SaveForLater::css/saveforlater.css" />
    </head> 
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Save For Later</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Yudiz\SaveForLater\Block\SaveForLater" name="custom_tab" template="Yudiz_SaveForLater::saveforlater.phtml"  cacheable="false"/>
        </referenceContainer>
    </body>
</page>